{
  "name": "x402-boilerplate",
  "version": "1.0.0",
  "description": "X402 Express.js boilerplate with TypeScript",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "call": "tsx tests/call.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "viem": "^2.29.1",
    "x402-axios": "^0.3.2",
    "x402-express": "^0.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  }
}
